{
    "name": "siworks/slim3-doctrine",
    "description": "Simple implementation of Slim3 with Doctrine",
    "keywords": ["microframework", "slim3", "doctrine", "rest", "router", "psr7"],
    "homepage": "https://github.com/siworksit/slim3-doctrine.git",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rafael N. Garbinatto",
            "email": "rafael@siworks.com.br",
            "homepage": "http://www.siworks.com.br/"
        },
        {
            "name": "Diego B. Pimentel",
            "email": "diego@siworks.com.br",
            "homepage": "http://www.siworks.com.br/"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/validator": "v5.4.8",
        "symfony/serializer-pack": "v1.1.0",
        "monolog/monolog": "^2.6.0",
        "doctrine/orm": "^2.12.2",
        "doctrine/common": "^3.3.0",
        "doctrine/cache": "^1.13.0",
        "doctrine/doctrine-bundle": "^2.6.3",
        "doctrine/migrations": "^2.3.5",
        "doctrine/doctrine-migrations-bundle": "^2.2.3",
        "ramsey/uuid-doctrine": "^1.8.1",
        "ocramius/generated-hydrator": "^4.1",
        "doctrine/doctrine-module": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "Siworks\\Slim\\Doctrine\\": "src/Doctrine/",
            "Siworks\\Slim\\Example\\": "src/Example/",
            "Siworks\\Slim\\Tests\\": "tests"
       }
    },
    "autoload-dev": {
        "psr-4": {
            "Siworks\\Slim\\Tests\\": "tests"
        }
    }
}
